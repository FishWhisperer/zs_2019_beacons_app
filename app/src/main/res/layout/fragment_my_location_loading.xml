<com.facebook.shimmer.ShimmerFrameLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/shimmer_view_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:shimmer_highlight_alpha="0"
        app:shimmer_highlight_color="@color/colorPrimary">

    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/linearLayout3"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                <ImageView
                        android:src="@drawable/moja_lokacija_privzeta_slika"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/imageView"
                        android:layout_marginEnd="16dp"
                        android:layout_marginTop="16dp"
                        android:layout_marginBottom="16dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:tint="@color/shimmerBackground"/>

                <View
                        android:layout_width="0dp"
                        android:background="@color/shimmerBackground"
                        android:layout_height="@dimen/viewHeightTextTop"
                        android:id="@+id/textViewNahajasSe"
                        android:layout_marginEnd="24dp"
                        android:layout_marginStart="16dp"
                        android:layout_marginBottom="8dp"
                        app:layout_constraintBottom_toTopOf="@+id/tvImeLokacije"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toStartOf="@+id/imageView"/>

                <View
                        android:layout_width="0dp"
                        android:background="@color/shimmerBackground"
                        android:layout_height="@dimen/viewHeightTextMid"
                        android:id="@+id/tvImeLokacije"
                        android:layout_marginEnd="24dp"
                        android:layout_marginStart="16dp"
                        android:layout_marginBottom="8dp"
                        app:layout_constraintEnd_toStartOf="@+id/imageView"
                        app:layout_constraintBottom_toTopOf="@+id/tvNadstropje"
                        app:layout_constraintStart_toStartOf="parent"/>

                <View
                        android:layout_width="0dp"
                        android:background="@color/shimmerBackground"
                        android:layout_height="@dimen/viewHeightTextBot"
                        android:id="@+id/tvNadstropje"
                        android:layout_marginEnd="24dp"
                        android:layout_marginBottom="16dp"
                        android:layout_marginStart="16dp"
                        app:layout_constraintEnd_toStartOf="@+id/imageView"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"/>

            </androidx.constraintlayout.widget.ConstraintLayout>

        </LinearLayout>

        <View
                android:id="@+id/divider6"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/mainDivider"
                android:layout_marginBottom="8dp"
                app:layout_constraintTop_toBottomOf="@+id/linearLayout3"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintBottom_toTopOf="@id/linearLayout4"
                android:layout_marginEnd="16dp"
                android:layout_marginStart="16dp"/>

        <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@id/divider6"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:id="@+id/linearLayout4">

            <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                <ImageView
                        android:src="@drawable/moja_lokacija_privzeta_slika"
                        android:id="@+id/imageView3"
                        android:layout_marginEnd="16dp"
                        android:layout_marginTop="32dp"
                        android:layout_marginBottom="24dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:tint="@color/shimmerBackground"
                        android:layout_height="@dimen/viewHeightListImage"
                        android:layout_width="@dimen/viewHeightListImage"/>

                <View
                        android:layout_width="0dp"
                        android:background="@color/shimmerBackground"
                        android:layout_height="@dimen/viewHeightListTop"
                        android:id="@+id/textView3"
                        android:layout_marginEnd="85dp"
                        android:layout_marginStart="16dp"
                        android:layout_marginBottom="8dp"
                        app:layout_constraintBottom_toTopOf="@+id/textView33"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toStartOf="@+id/imageView3" app:layout_constraintTop_toTopOf="parent"
                        android:layout_marginTop="24dp"/>

                <View
                        android:layout_width="0dp"
                        android:background="@color/shimmerBackground"
                        android:layout_height="@dimen/viewHeightListMid"
                        android:id="@+id/textView33"
                        android:layout_marginEnd="32dp"
                        android:layout_marginStart="16dp"
                        android:layout_marginBottom="8dp"
                        app:layout_constraintEnd_toStartOf="@+id/imageView3"
                        app:layout_constraintBottom_toTopOf="@+id/textView333"
                        app:layout_constraintStart_toStartOf="parent"/>

                <View
                        android:layout_width="0dp"
                        android:background="@color/shimmerBackground"
                        android:layout_height="@dimen/viewHeightListBot"
                        android:id="@+id/textView333"
                        android:layout_marginEnd="32dp"
                        android:layout_marginBottom="16dp"
                        android:layout_marginStart="16dp"
                        app:layout_constraintEnd_toStartOf="@+id/imageView3"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"/>

            </androidx.constraintlayout.widget.ConstraintLayout>
            <View
                    android:id="@+id/divider5"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/mainDivider"
                    android:layout_marginStart="16dp" android:layout_marginEnd="16dp"/>
            <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                <ImageView
                        android:src="@drawable/moja_lokacija_privzeta_slika"
                        android:id="@+id/recycler_img"
                        android:layout_marginEnd="16dp"
                        android:layout_marginTop="32dp"
                        android:layout_marginBottom="24dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:tint="@color/shimmerBackground"
                        android:layout_height="@dimen/viewHeightListImage"
                        android:layout_width="@dimen/viewHeightListImage"/>

                <View
                        android:layout_width="0dp"
                        android:background="@color/shimmerBackground"
                        android:layout_height="@dimen/viewHeightListTop"
                        android:id="@+id/textView2"
                        android:layout_marginEnd="85dp"
                        android:layout_marginStart="16dp"
                        android:layout_marginBottom="8dp"
                        app:layout_constraintBottom_toTopOf="@+id/recycler_tv_mid"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toStartOf="@+id/recycler_img"
                        app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="24dp"/>

                <View
                        android:layout_width="0dp"
                        android:background="@color/shimmerBackground"
                        android:layout_height="@dimen/viewHeightListMid"
                        android:id="@+id/recycler_tv_mid"
                        android:layout_marginEnd="32dp"
                        android:layout_marginStart="16dp"
                        android:layout_marginBottom="8dp"
                        app:layout_constraintEnd_toStartOf="@+id/recycler_img"
                        app:layout_constraintBottom_toTopOf="@+id/recycler_tv_bot"
                        app:layout_constraintStart_toStartOf="parent"/>

                <View
                        android:layout_width="0dp"
                        android:background="@color/shimmerBackground"
                        android:layout_height="@dimen/viewHeightListBot"
                        android:id="@+id/recycler_tv_bot"
                        android:layout_marginEnd="32dp"
                        android:layout_marginBottom="16dp"
                        android:layout_marginStart="16dp"
                        app:layout_constraintEnd_toStartOf="@+id/recycler_img"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"/>

            </androidx.constraintlayout.widget.ConstraintLayout>
            <View
                    android:id="@+id/divider4"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/mainDivider"
                    android:layout_marginStart="16dp" android:layout_marginEnd="16dp"/>
            <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                <ImageView
                        android:src="@drawable/moja_lokacija_privzeta_slika"
                        android:id="@+id/imageView1"
                        android:layout_marginEnd="16dp"
                        android:layout_marginTop="32dp"
                        android:layout_marginBottom="24dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:tint="@color/shimmerBackground"
                        android:layout_height="@dimen/viewHeightListImage"
                        android:layout_width="@dimen/viewHeightListImage"/>

                <View
                        android:layout_width="0dp"
                        android:background="@color/shimmerBackground"
                        android:layout_height="@dimen/viewHeightListTop"
                        android:id="@+id/textView1"
                        android:layout_marginEnd="85dp"
                        android:layout_marginStart="16dp"
                        android:layout_marginBottom="8dp"
                        app:layout_constraintBottom_toTopOf="@+id/textView11"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toStartOf="@+id/imageView1"
                        app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="24dp"/>

                <View
                        android:layout_width="0dp"
                        android:background="@color/shimmerBackground"
                        android:layout_height="@dimen/viewHeightListMid"
                        android:id="@+id/textView11"
                        android:layout_marginEnd="32dp"
                        android:layout_marginStart="16dp"
                        android:layout_marginBottom="8dp"
                        app:layout_constraintEnd_toStartOf="@+id/imageView1"
                        app:layout_constraintBottom_toTopOf="@+id/textView111"
                        app:layout_constraintStart_toStartOf="parent"/>

                <View
                        android:layout_width="0dp"
                        android:background="@color/shimmerBackground"
                        android:layout_height="@dimen/viewHeightListBot"
                        android:id="@+id/textView111"
                        android:layout_marginEnd="32dp"
                        android:layout_marginBottom="16dp"
                        android:layout_marginStart="16dp"
                        app:layout_constraintEnd_toStartOf="@+id/imageView1"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"/>

            </androidx.constraintlayout.widget.ConstraintLayout>
            <View
                    android:id="@+id/divider3"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/mainDivider"
                    android:layout_marginStart="16dp" android:layout_marginEnd="16dp"/>
        </LinearLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</com.facebook.shimmer.ShimmerFrameLayout>